PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX f: <http://www.semanticweb.org/birdb/ontologies/2025/8/èŒ¶#>

# Query Drink without TeaLeaf as its ingredient
SELECT DISTINCT ?tea
WHERE {
    ?tea rdf:type f:Tea.
    MINUS {
        ?tea f:hasIngredient ?ingredient.
        ?ingredient rdf:type f:TeaLeaf.
    }
}
