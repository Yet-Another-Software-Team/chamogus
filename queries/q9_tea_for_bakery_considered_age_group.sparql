PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX f: <http://www.semanticweb.org/birdb/ontologies/2025/8/èŒ¶#>

# Find Tea that complement Apple Pie which preferred by Teen
SELECT DISTINCT ?tea
WHERE {
    f:Teen f:hasPreference ?prefChar.
    ?tea rdf:type f:Tea;
        f:goWellWithBakery f:ApplePie.
    {
        { ?tea f:hasAroma ?prefChar. }
        UNION
        { ?tea f:hasFlavour ?prefChar.}
    }
}
