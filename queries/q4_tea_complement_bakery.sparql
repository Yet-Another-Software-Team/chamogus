PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX f: <http://www.semanticweb.org/birdb/ontologies/2025/8/èŒ¶#>

# Find Tea that complement specified bakery
SELECT DISTINCT ?tea ?flavour ?aroma
WHERE {
    ?bakery rdfs:label f: ApplePie.
    ?tea rdf:type f:Tea;
        f:goWellWithBakery ?bakery;
        f:hasFlavour ?flavour;
        f:hasAroma ?aroma.
}